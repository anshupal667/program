    <script>
         var t1 = 0,  t2 = 1, next_num, i;  
         var num = parseInt (prompt (" Enter the limit for Fibonacci Series "));  
       document.write( "Fibonacci Series: ");  
      for ( i = 1; i <= num; i++)  
{   if(t1<num)
    document.write (" <br> " +  t1); // print the t1  
    next_num = t1 + t2; // sum of n1 and n2 into the next_num  
      
    t1 = t2; // assign the n2 value into n2  
    t2 = next_num; // assign the next_num into n2  
}  
    </script>
